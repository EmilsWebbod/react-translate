{
  "name": "@ewb/react-translate",
  "version": "1.2.2",
  "description": "Translate as you create your react app.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "yarn build:watch",
    "build": "tsc",
    "build:watch": "tsc --watch --incremental",
    "lint": "tslint --project \"./tsconfig.json\"",
    "test": "mocha --watch --watch-extensions ts -r ts-node/register test/**/*.spec.ts",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run build",
    "preversion": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmilsWebbod/react-translate.git"
  },
  "keywords": [
    "typescript",
    "npm",
    "translate",
    "tree",
    "easy",
    "react"
  ],
  "author": "Emils Webbod",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmilsWebbod/react-translate/issues"
  },
  "homepage": "https://github.com/EmilsWebbod/react-translate#readme",
  "files": [
    "lib",
    "server.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "google-translate": "^3.0.0",
    "multer": "^1.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "@ewb/translate": "^1.3.1"
  }
}
