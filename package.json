{
  "name": "@ewb/react-translate",
  "version": "1.9.1",
  "description": "Translate as you create your react app.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "yarn build:watch",
    "build": "tsc",
    "build:watch": "tsc --watch --incremental",
    "lint": "tslint --project \"./tsconfig.json\"",
    "test": "mocha --watch --watch-extensions ts -r ts-node/register test/**/*.spec.ts",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run build",
    "preversion": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmilsWebbod/react-translate.git"
  },
  "keywords": [
    "typescript",
    "npm",
    "translate",
    "tree",
    "easy",
    "react"
  ],
  "author": "Emils Webbod",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmilsWebbod/react-translate/issues"
  },
  "homepage": "https://github.com/EmilsWebbod/react-translate#readme",
  "files": [
    "lib",
    "src",
    "server.js"
  ],
  "devDependencies": {
    "@ewb/translate": "^1.8.1",
    "@types/file-saver": "^2.0.3",
    "@types/json2csv": "^5.0.3",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "multer": "^1.4.3",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@ewb/translate": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "google-translate": "^3.0.0",
    "json2csv": "^5.0.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
