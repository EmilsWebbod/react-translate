{
  "name": "@ewb/react-translate",
  "version": "2.0.1-beta",
  "type": "module",
  "description": "Translate as you create your react app.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "yarn build:watch",
    "build": "tsc",
    "build:watch": "tsc --watch --incremental",
    "lint": "tslint --project \"./tsconfig.json\"",
    "test": "mocha --watch --watch-extensions ts -r ts-node/register test/**/*.spec.ts",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run build",
    "preversion": "npm run build"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "csvtojson": "^2.0.10",
    "express": "^4.18.2",
    "google-translate": "^3.0.0",
    "json2csv": "^5.0.7"
  },
  "devDependencies": {
    "@ewb/translate": "^2.0.2",
    "@types/file-saver": "^2.0.5",
    "@types/json2csv": "^5.0.3",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "multer": "^1.4.5-lts.1",
    "prettier": "^2.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "@ewb/translate": "*",
    "react": "*",
    "react-dom": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmilsWebbod/react-translate.git"
  },
  "keywords": [
    "typescript",
    "npm",
    "translate",
    "tree",
    "easy",
    "react"
  ],
  "author": "Emils Webbod",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmilsWebbod/react-translate/issues"
  },
  "homepage": "https://github.com/EmilsWebbod/react-translate#readme",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
